<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="2.8.3">
		<id>Plugin.FelicaReader</id>
		<version>1.0.0</version>
		<title>FelicaReader Plugin for Xamarin and UWP</title>
		<authors>Aoki Nobuaki</authors>
		<owners>Aoki Nobuaki</owners>
    <licenseUrl>https://github.com/nobukuma/FelicaReader/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/nobukuma/FelicaReader</projectUrl>
		<iconUrl>https://raw.githubusercontent.com/nobukuma/FelicaReader/master/noun_775357.png</iconUrl>

		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
         Felica card reader plugins for Xamarin.Android and Xamarin.UWP.
         With this plugin, the app can detect a Felica card (such as Suica, PASMO and Edy) and send Felica commands to it.
         
         The icon image is "Card Nfc" by Viktor Vorobyev from the Noun Project (https://thenounproject.com/term/card-nfc/775357/).
		</description>
		<summary>Felica card reader plugins for Xamarin.Android and Xamarin.UWP</summary>
		<tags>plugin for xamarin, uwp, android, felica</tags>
		<dependencies>
			<group targetFramework="MonoAndroid10">
         <dependency id="System.Reactive" version="[3.1.1,)" />
			</group>
			<group targetFramework="UAP10.0">
         <dependency id="System.Reactive" version="[3.1.1,)" />
			</group>
      <group targetFramework="netstandard1.4">
      </group>
		</dependencies>
	</metadata>
